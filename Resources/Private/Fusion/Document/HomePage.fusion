prototype(NeosRulez.Neos.Tailwind.NodeTypes:Document.HomePage) < prototype(NeosRulez.Neos.Tailwind:Document.AbstractPage) {
    body {
        content = Neos.Fusion:Component {
            main = Neos.Neos:ContentCollection {
                nodePath = 'main'
            }
            renderer = afx`
                <main id="homepage" class="main">
                    {props.main}
                </main>
            `
        }
    }
}
